/* Header */

#header h1 { margin: 0; padding: 30px 0 15px; line-height: 1.0em;}

#header h2 { margin: 0; padding: 0 0 30px; text-transform: uppercase; font-size: 13px; color: #888888; line-height: 1.0em; }

/* Footer */

