<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tssa.km.domain.hibernate">

	<class name="QuestionType" table="kmp_questions_type">

		<id name="id" column="id" type="long" unsaved-value="null">
			<generator class="native" />
		</id>

		<version
			name="version"
			column="version"
			type="integer"
			/>
			
        <property
            name="name"
            length="255"
            not-null="true"
            />

		<property
			name="description"
			length="255"
			not-null="true"
			/>

		<property
			name="createdTs"
			access="field"
			type="timestamp"
			update="false"
			/>

	</class>

	<query name="question.type.listAll">
		from QuestionType
	</query>
</hibernate-mapping>